{
  "name": "bookmyshow-client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Frontend for BookMyShow-style movie ticket booking app built with React, Vite, TypeScript, TailwindCSS, and Clerk authentication.",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "preview": "vite preview",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.32.0",
    "@tailwindcss/vite": "^4.1.10",
    "axios": "^1.10.0",
    "lucide-react": "^0.515.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-player": "^2.16.0",
    "react-router-dom": "^7.6.2",
    "tailwindcss": "^4.1.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.0.4"
  }
}
